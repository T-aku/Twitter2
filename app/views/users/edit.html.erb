<div class="row rowver2">
  <div class="col-7 index-center">
    <%= form_with model: @user do |f| %>
    <div class="form-all" align="center">
      <i class="fa fa-twitter twitter" aria-hidden="true"></i>
      <ul class="form-contents">
        <li><%= f.text_field :username, class: "form-input", placeholder: ' Edit name', autofocus: true %></li>
        <li><%= f.password_field :password, class: "form-input", placeholder: ' Edit password' %></li>
        <li><%= f.file_field :image, class: "form-input-file" %></li>
        <li><%= f.submit("Update", class: "form-submit") %></li>
      </ul>
    </div>
    <% end %>
  </div>
  <div class="col-5 index-right">
    <div class="right-sidebar">
    </div>
  </div>
</div>
